
<li class="list-group">
  <div>
 	<span class="title"><%= link_to ticket.title + " ##{ticket.id}",ticket %></span>

	Issue Priority:
	  	<span class="<%="badge alert#{issue_type_badge ticket.priority.name}"%>" >
	          <%=ticket.priority.name%> 
	    </span>
  
	| Ticket State:
    <span class="state">
    <%= ticket.state.name%>
    </span>
    <%if ticket.assigned?%>
    	| <span class="badge alert-info"> 
         "Assigned"
    	 </span>
    	<%else%>
    	|  <span class="badge alert-warning"> 
          "Not Assigned"
    	 </span>
    <%end%>
    </br>
    <span class="timestamp">
    	Updated <%= time_ago_in_words(ticket.updated_at) %> ago |
  	</span>
    <span class="timestamp">
    	Posted <%= time_ago_in_words(ticket.created_at) %> ago.
  	</span>
  </row>
</li>

